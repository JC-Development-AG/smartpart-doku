<Text><![CDATA[! 2D CAVIC Rückbiegeanschluss der Fa. AVI
! (c) 2012, Manfred Moitzi, smartparts.tzb-moitzi.at
! License: GPLv3, www.fsf.org

VALUES "view" "PLAN", "VIEW", "SECTION"
VALUES "type" "C 8/20", "C 8/15", "C 10/20", "C 10/15", "C 12/20", "C 12/15"
VALUES "ref_x" RANGE [0.625, ] STEP 0.625, 0.625

HIDE "ref_y", "ref_z"
]]></Text>
</PartParameter>
<PartMaster>
<Enable>0</Enable>
<Text><![CDATA[]]></Text>
</PartMaster>
<Part2D>
<Enable>1</Enable>
<Text><![CDATA[! 2D CAVIC Rückbiegeanschluss der Fa. AVI
! (c) 2012, Manfred Moitzi, smartparts.tzb-moitzi.at
! License: GPLv3, www.fsf.org

! box data
!---------
length = ref_x
width = 0.067
width2 = width / 2
thickness = 0.035
back_length = 0.15 - thickness
rebar_bending_radius = 0.025
id = 1 ! handle id

LAYER "BP_BEW"

GOSUB type
GOSUB representation
END

"PLAN":
!------

HANDLE2 (0), (0), id, "ref_x", 1
HANDLE2 (0+ref_x), (0), id, "ref_x", 2
HANDLE2 (-1), (0), id, "ref_x", 3
id = id + 1

WHILE length > 0 DO
IF length > 0.625 THEN
    box_length = 1.25
    draw_rebars = rebar_count
ELSE
    box_length = 0.625
    draw_rebars = rebar_count / 2
ENDIF

! draw cavic box
!---------------
PEN cavic_pen
STROKE cavic_stroke
RECT2 0, (-width2), (box_length), (width2)


! draw rebars
!------------
rebar_x = (1.25 - (rebar_count-1) * pitch) / 2

FOR i = 1 to draw_rebars
    CIRCLE2 rebar_x, 0, 0.006
    rebar_x = rebar_x + pitch
NEXT i
TRANS2 box_length, 0
length = length - box_length
ENDWHILE

RETURN

"VIEW":
!------

HANDLE2 (0), (0), id, "ref_x", 1
HANDLE2 (0+ref_x), (0), id, "ref_x", 2
HANDLE2 (-1), (0), id, "ref_x", 3
id = id + 1

WHILE length > 0 DO
IF length > 0.625 THEN
    box_length = 1.25
    draw_rebars = rebar_count
ELSE
    box_length = 0.625
    draw_rebars = rebar_count / 2
ENDIF

! draw cavic box
!---------------
PEN cavic_pen
STROKE cavic_stroke
RECT2 0, (0), (box_length), (thickness)


! draw rebars
!------------
rebar_x = (1.25 - (rebar_count-1) * pitch) / 2
FOR i = 1 to draw_rebars
    STROKE rebar_stroke
    LINE2 rebar_x, thickness, rebar_x, thickness + rebar_length
    STROKE cavic_stroke
IF i > (rebar_count / 2) THEN
    angle = -180
ELSE
    angle = 180
ENDIF
POLY2_ 4, 1,
(rebar_x), (0), (0),
(0), (rebar_bending_radius-back_length), (100+0),
(rebar_bending_radius), (angle), (2000+0),
(0), (0.05), (100+0)
    rebar_x = rebar_x + pitch
NEXT i

TRANS2 box_length, 0
length = length - box_length

ENDWHILE

RETURN

"SECTION":
!---------


! draw cavic box
!---------------
PEN cavic_pen
STROKE cavic_stroke
RECT2 0, (-width2), (thickness), (width2)


! draw rebar
!-----------
STROKE rebar_stroke
LINE2 -back_length, 0, thickness + rebar_length, 0

RETURN


"C 8/20":
pitch = 0.2
rebar_length = 0.3
rebar_count = 6
RETURN


"C 8/15":
pitch = 0.15
rebar_length = 0.3
rebar_count = 8
RETURN


"C 10/20":
pitch = 0.2
rebar_length = 0.38
rebar_count = 6
RETURN


"C 10/15":
pitch = 0.15
rebar_length = 0.38
rebar_count = 8
RETURN


"C 12/20":
pitch = 0.2
rebar_length = 0.45
rebar_count = 6
RETURN


"C 12/15":
pitch = 0.15
rebar_length = 0.45
rebar_count = 8
RETURN














]]></Text>
