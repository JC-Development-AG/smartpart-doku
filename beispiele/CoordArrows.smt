<Text><![CDATA[DEFINE STYLE "ArrowText" "Arial" , text_height , 6 , 1 
STYLE "ArrowText" 
PEN 1 
STROKE 1 

GOSUB "draw_cube" 
GOSUB "draw_x_arrow" 
GOSUB "draw_y_arrow" 
GOSUB "draw_z_arrow" 
REF_HANDLES
END 

"draw_cube": 
COLOR 1 
TRANS -origin_cube_size / 2 , -origin_cube_size / 2 , -origin_cube_size / 2 
BOX origin_cube_size , origin_cube_size , origin_cube_size 
RESTORE 1 

"draw_x_arrow": 
arrow_text = "X" 
arrow_length = ref_x - arrow_head_length - origin_cube_size / 2
COLOR 6 
ROTY 90
GOSUB "draw_arrow" 
RESTORE 1
RETURN 

"draw_y_arrow": 
arrow_text = "Y" 
arrow_length = ref_y - arrow_head_length - origin_cube_size / 2
COLOR 4 
ROTX -90
GOSUB "draw_arrow" 
RESTORE 1
RETURN 

"draw_z_arrow": 
arrow_text = "Z" 
arrow_length = ref_z - arrow_head_length - origin_cube_size / 2
COLOR 7 
GOSUB "draw_arrow" 
RETURN 

"draw_arrow": 
TRANSZ origin_cube_size / 2 
CYLIND arrow_length , arrow_diameter / 2 
TRANSZ arrow_length 
CONE arrow_head_length , arrow_head_diameter / 2 , 0.001 , 90 , 90 
TRANS 0, -arrow_diameter / 2, arrow_head_length * 1.1
ROTZ 90
ROTY 90
TEXT arrow_diameter , 0 , arrow_text 
RESTORE 5
RETURN ]]></Text>
</Part3D>
