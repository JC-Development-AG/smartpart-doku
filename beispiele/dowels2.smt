<Script><PartParameter>
<Enable>1</Enable>
<Text><![CDATA[! set reference-box parameters

VALUES "dowel_diameter" RANGE [ 8 , 50 ] 
VALUES "dowel_count" RANGE [ 1 , ) STEP 1, 1 
VALUES "dowel_length" RANGE [ 0.05 , ) STEP 0.05, 0.01 
VALUES "ref_x" RANGE [dowel_distance, ) STEP dowel_distance, dowel_distance
VALUES "view_type" "PLAN", "VIEW", "SECTION"
PARAMETERS REF_Y = plate_width , REF_Z = dowel_length ]]></Text>
</PartParameter>
<PartMaster>
<Enable>1</Enable>
<Text><![CDATA[]]></Text>
</PartMaster>
<Part2D>
<Enable>1</Enable>
<Text><![CDATA[! (C) 2012, Manfred Moitzi, smartparts.tzb-moitzi.at
! License: General Public License v3 (GPLv3), www.fsf.org

RESOL 12
COLOR dowel_color
PEN dowel_pen
LAYER dowel_layer

dowel_count = INT(REF_X / dowel_distance)
plate_extension = dowel_distance / 2
plate_width2 = plate_width / 2 
dowel_length2 = dowel_length / 2
dowel_diameter = dowel_diameter / 1000 ! convert to [m]
dowel_radius = dowel_diameter / 2 
id = 1

GOSUB view_type
END 

"PLAN":
RECT2 0 , -plate_width2 , REF_X, plate_width2
FOR i = 0 TO (dowel_count-1)
   CIRCLE2 plate_extension + (i * dowel_distance), 0, dowel_radius
NEXT i
GOSUB "place_plate_handle"
RETURN

"VIEW":
RECT2 0 , 0, REF_X, plate_thickness
FOR i = 0 TO (dowel_count-1)
   x1 = plate_extension + (i * dowel_distance) - dowel_radius
   x2 = x1 + dowel_diameter
   RECT2 x1, -dowel_length2, x2, dowel_length2
NEXT i
GOSUB "place_plate_handle"
TRANS2 plate_extension, 0
GOSUB "place_dowel_handle"
RESTORE 1
RETURN

"SECTION":
RECT2 -plate_width2 , 0, plate_width2, plate_thickness
RECT2 -dowel_radius, -dowel_length2, dowel_radius, dowel_length2
GOSUB "place_dowel_handle"
RETURN

"place_plate_handle":
HANDLE2 0, 0, id, ref_x, 1
HANDLE2 ref_x, 0, id, ref_x, 2
HANDLE2 -1, 0, id, ref_x, 3
id = id + 1
RETURN

"place_dowel_handle":
HANDLEFACTOR 2
HANDLE2 0, 0, id, dowel_length, 1
HANDLE2 0, dowel_length2, id, dowel_length, 2
HANDLE2 0, -1, id, dowel_length, 3
HANDLEFACTOR 1
id = id + 1
RETURN]]></Text>
</Part2D>
