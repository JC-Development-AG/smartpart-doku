I_PALETTE `2D Dübelleiste`, 1,
"(c) 2012 Manfred Moitzi", "link=http://smartparts.tzb-moitzi.at TZBM",
"General Public License v3 (GPLv3)", "link=http://www.gnu.org/licenses/ Lizenz"

I_SECTION `Flachstahl`
I_INFIELD "ref_x", 0, 0, 1, 1 I_TOOLTIP `Muss ein Vielfaches des Dübelabstandes sein!`
I_INFIELD "plate_thickness", 0, 0, 1, 1
I_INFIELD "plate_width", 0, 0, 1, 1

I_SECTION `Darstellung`
I_RADIOBUTTON "view_type" , "PLAN" , `Grundriss` , 0 , 0 , 0 , 0
I_RADIOBUTTON "view_type" , "VIEW" , `Ansicht` , 0 , 0 , 0 , 0
I_RADIOBUTTON "view_type" , "SECTION" , `Schnitt` , 0 , 0 , 0 , 0

I_SECTION `Dübel`
I_INFIELD "dowel_diameter", 0, 0, 1, 1 I_TOOLTIP `Dübeldurchmesser in [mm]`
I_INFIELD "dowel_distance", 0, 0, 1, 1
I_INFIELD "dowel_length", 0, 0, 1, 1

I_SECTION `Format`
I_INFIELD "dowel_color", 0, 0, 1, 1
I_INFIELD "dowel_pen", 0, 0, 1, 1
I_INFIELD "dowel_layer", 0, 0, 1, 1
]]></Text>
