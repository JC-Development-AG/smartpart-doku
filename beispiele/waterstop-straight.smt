VALUES "ws_width" RANGE [0.16, 0.32] STEP 0.16, 0.01
VALUES "ws_cut_angle_1" RANGE [-89, 89]
VALUES "ws_cut_angle_2" RANGE [-89, 89]]]></Text>
</PartParameter>
<PartMaster>
<Enable>0</Enable>
<Text><![CDATA[]]></Text>
</PartMaster>
<Part2D>
<Enable>0</Enable>
<Text><![CDATA[]]></Text>
</Part2D>
<Part3D>
<Enable>1</Enable>
<Text><![CDATA[! waterstop-straight
! (c) 2012, Manfred Moitzi, smartparts.tzb-moitzi.at
! License: GPLv3, www.fsf.org



IF ws_is_expansion_joint THEN 
	ws_name = "expansion_joint"
ELSE
	ws_name = "construction_joint"
ENDIF

COLOR ws_color
LAYER ws_layer
PEN ws_pen

GOSUB "calc_extensions"
total_length = ws_bottom_ext + REF_Z + ws_top_ext

GOSUB "setup_"+ws_name
GOSUB "put_profile_for_"+ws_name

ROTZ 90 ! cutplane rotates about x-axis, we need y-axis
CUTPLANE ws_cut_angle_1 + 180 ! bottom cut, +180 to remove all BELOW the cutplane
TRANSZ REF_Z ! move up for top cut
CUTPLANE ws_cut_angle_2 ! top cut
RESTORE 2 ! reset to origin
	TRANSZ -ws_bottom_ext
	PRISM_ 0, total_length, GET(NUM_SP)
	RESTORE 1 ! reset to origin
CUTEND ! top cut
CUTEND ! bottom cut

GOSUB "place_handles"
END

"calc_extensions":
ws_bottom_ext = ABS((width / 2) * TAN(ws_cut_angle_1))
ws_top_ext = ABS((width / 2) * TAN(ws_cut_angle_2))
RETURN

"setup_construction_joint":

waterstop_height = width
waterstop_thickness = 0.005
waterstop_d = (waterstop_height/4) - (waterstop_thickness * 0.5)
waterstop_e = (waterstop_height/4) - (waterstop_thickness * 1.5)

RETURN

"put_profile_for_construction_joint":

__start_x__ = (0-waterstop_d)
__start_y__ = (0+waterstop_thickness/2)
PUT __start_x__, __start_y__, 0, ! Nr. 1
0, waterstop_thickness, 100, ! Nr. 2
-waterstop_thickness, 0, 100, ! Nr. 3
0, -waterstop_thickness, 100, ! Nr. 4
-waterstop_e, 0, 100, ! Nr. 5
0, waterstop_thickness, 100, ! Nr. 6
-waterstop_thickness, 0, 100, ! Nr. 7
0, -3*waterstop_thickness, 100, ! Nr. 8
waterstop_thickness, 0, 100, ! Nr. 9
0, waterstop_thickness, 100, ! Nr. 10
waterstop_e, 0, 100, ! Nr. 11
0, -waterstop_thickness, 100, ! Nr. 12
waterstop_thickness, 0, 100, ! Nr. 13
0, waterstop_thickness, 100, ! Nr. 14
2*waterstop_d, 0, 100, ! Nr. 15
0, -waterstop_thickness, 100, ! Nr. 16
waterstop_thickness, 0, 100, ! Nr. 17
0, waterstop_thickness, 100, ! Nr. 18
waterstop_e, 0, 100, ! Nr. 19
0, -waterstop_thickness, 100, ! Nr. 20
waterstop_thickness, 0, 100, ! Nr. 21
0, 3*waterstop_thickness, 100, ! Nr. 22
-waterstop_thickness, 0, 100, ! Nr. 23
0, -waterstop_thickness, 100, ! Nr. 24
-waterstop_e, 0, 100, ! Nr. 25
0, waterstop_thickness, 100, ! Nr. 26
-waterstop_thickness, 0, 100, ! Nr. 27
0, -waterstop_thickness, 100, ! Nr. 28
__start_x__, __start_y__, 0 ! Nr. 1

RETURN

"setup_expansion_joint":

waterstop_height = width
waterstop_width = 0.03
waterstop_width2 = waterstop_width / 2
waterstop_thickness = 0.005
waterstop_a = 0.005
waterstop_b = waterstop_width2 - waterstop_a
waterstop_c = waterstop_width2 - (waterstop_a/2)
waterstop_d = (waterstop_height/4) - waterstop_width2 - (waterstop_thickness/2)
waterstop_e = (waterstop_height/4) - (waterstop_thickness * 1.5)

RETURN

"put_profile_for_expansion_joint":

__start_x__ = (0+waterstop_b)
__start_y__ = (0+waterstop_width2)
PUT __start_x__, __start_y__, 0, ! Nr. 1
-2*waterstop_b, 0, 100, ! Nr. 2
-waterstop_a, -waterstop_c, 100, ! Nr. 3
-waterstop_d, 0, 100, ! Nr. 4
0, waterstop_thickness, 100, ! Nr. 5
-waterstop_thickness, 0, 100, ! Nr. 6
0, -waterstop_thickness, 100, ! Nr. 7
-waterstop_e, 0, 100, ! Nr. 8
0, waterstop_thickness, 100, ! Nr. 9
-waterstop_thickness, 0, 100, ! Nr. 10
0, -3*waterstop_thickness, 100, ! Nr. 11
waterstop_thickness, 0, 100, ! Nr. 12
0, waterstop_thickness, 100, ! Nr. 13
waterstop_e, 0, 100, ! Nr. 14
0, -waterstop_thickness, 100, ! Nr. 15
waterstop_thickness, 0, 100, ! Nr. 16
0, waterstop_thickness, 100, ! Nr. 17
waterstop_d, 0, 100, ! Nr. 18
waterstop_a, -waterstop_c, 100, ! Nr. 19
2*waterstop_b, 0, 100, ! Nr. 20
waterstop_a, waterstop_c, 100, ! Nr. 21
waterstop_d, 0, 100, ! Nr. 22
0, -waterstop_thickness, 100, ! Nr. 23
waterstop_thickness, 0, 100, ! Nr. 24
0, waterstop_thickness, 100, ! Nr. 25
waterstop_e, 0, 100, ! Nr. 26
0, -waterstop_thickness, 100, ! Nr. 27
waterstop_thickness, 0, 100, ! Nr. 28
0, 3*waterstop_thickness, 100, ! Nr. 29
-waterstop_thickness, 0, 100, ! Nr. 30
0, -waterstop_thickness, 100, ! Nr. 31
-waterstop_e, 0, 100, ! Nr. 32
0, waterstop_thickness, 100, ! Nr. 33
-waterstop_thickness, 0, 100, ! Nr. 34
0, -waterstop_thickness, 100, ! Nr. 35
-waterstop_d, 0, 100, ! Nr. 36
__start_x__, __start_y__, 0 ! Nr. 1

RETURN

"place_handles":
id = 1
HANDLE (0), (0), (0), id, ref_z, 1
HANDLE (0), (0), (ref_z), id, ref_z, 2
HANDLE (0), (0), (-1), id, ref_z, 3
RETURN]]></Text>
